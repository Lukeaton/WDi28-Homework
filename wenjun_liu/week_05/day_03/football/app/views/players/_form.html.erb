<%= form_for @player do |p| %>
  <%= p.label :Name %>
  <%= p.text_field :name %>

  <%= p.label :Nationality %>
  <%= p.text_field :nationality %>

  <%= p.label :Position%>
  <%= p.text_field :position %>

  <%= p.label :dob, "Date of Birth" %>
  <%= p.date_field :dob %>

  <%= p.label :Club %>
  <%= p.select :club_id, Club.pluck(:name, :id), :include_blank => true %>

  <%= p.submit %>
<% end %>
